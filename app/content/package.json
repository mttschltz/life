{
  "name": "content",
  "version": "0.0.1",
  "private": true,
  "description": "Adds content to Store",
  "author": {
    "name": "Matt Schultz",
    "email": "hey@matt.fyi",
    "url": "https://www.matt.fyi"
  },
  "scripts": {
    "build": "webpack --config webpack.production.js",
    "build:dev": "webpack --config webpack.development.js",
    "build:dev-watch": "webpack --env watch=true --config webpack.development.js",
    "codegen": "yarn apollo codegen:generate src/__generated__ --outputFlat --localSchemaFile=../../lib/life/src/api/graph/schema.graphql --target=typescript --tagName=gql",
    "codegen:watch": "yarn apollo codegen:generate src/__generated__ --outputFlat --localSchemaFile=../../lib/life/src/api/graph/schema.graphql --target=typescript --tagName=gql --watch",
    "develop": "yarn build:dev && concurrently \"yarn build:dev-watch\" \"yarn start\"",
    "start": "cross-env NODE_ENV=production node dist/server"
  },
  "devDependencies": {
    "@apollo/client": "3.3.21",
    "cross-fetch": "3.1.4"
  }
}
